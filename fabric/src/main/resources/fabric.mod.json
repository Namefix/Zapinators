  "authors": [
    "Namefix"
  ],
  "contact": {
    "homepage": "https://github.com/Namefix/Zapinators",
    "sources": "https://github.com/Namefix/Zapinators",
    "issues": "https://github.com/Namefix/Zapinators/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/zapinators/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.namefix.fabric.datagen.ZapinatorsModDataGenerator"
    ],
    "main": [
      "com.namefix.fabric.ZapinatorsModFabric"
    ],
    "client": [
      "com.namefix.fabric.client.ZapinatorsModFabricClient"
    ]
  },
  "mixins": [
    "zapinators.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "resourcefulconfig": ">=3.0.9",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
